{
    "name": "front",
    "version": "0.0.1",
    "scripts": {
        "prepare": "npm run smui-theme",
        "smui-theme": "smui-theme compile static/smui.css -i src/theme",
        "deploy": "prisma migrate deploy",
        "generate": "$npm_execpath run generate:prima && $npm_execpath run generate:houdini",
        "generate:houdini": "houdini generate",
        "generate:prima": "prisma generate && ./generated-args.sh",
        "sync": "svelte-kit sync",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "clean-frosbite": "NODE_ENV=production npx tailwindcss -o flowbite.css",
        "generate:request": "graphql-codegen --config codegen.yml",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
        "format": "prettier --plugin-search-dir . --write ."
    },
    "dependencies": {
        "@envelop/graphql-jit": "4.6.0",
        "@fightmegg/riot-api": "^0.0.8",
        "@prisma/client": "^4.3.1",
        "date-fns": "^2.29.3",
        "graphql": "^15.8.0",
        "graphql-yoga": "^3.0.0-next.4",
        "lodash": "^4.17.21",
        "reflect-metadata": "^0.1.13",
        "svelte-forms-lib": "^2.0.1",
        "type-graphql": "^1.1.1",
        "winston": "^3.8.2",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@fontsource/fira-mono": "^4.5.0",
        "@material/typography": "^14.0.0",
        "@neoconfetti/svelte": "^1.0.0",
        "@originjs/vite-plugin-commonjs": "^1.0.3",
        "@popperjs/core": "^2.11.6",
        "@smui-extra/autocomplete": "^7.0.0-beta.0",
        "@smui/button": "^7.0.0-beta.0",
        "@smui/card": "^7.0.0-beta.0",
        "@smui/circular-progress": "^7.0.0-beta.0",
        "@smui/fab": "^7.0.0-beta.0",
        "@smui/textfield": "^7.0.0-beta.0",
        "@sveltejs/adapter-auto": "1.0.0-next.83",
        "@sveltejs/adapter-vercel": "next",
        "@sveltejs/kit": "1.0.0-next.516",
        "@types/cookie": "^0.5.1",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "autoprefixer": "^10.4.7",
        "classnames": "^2.3.2",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "graphql": "^15.5.0",
        "houdini": "^0.17.4",
        "houdini-svelte": "^0.17.4",
        "postcss": "^8.4.14",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.7.0",
        "prisma": "^4.3.1",
        "puppeteer": "18.2.1",
        "smui-theme": "^7.0.0-beta.0",
        "svelte": "^3.46.0",
        "svelte-check": "^2.7.1",
        "svelte-heros-v2": "^0.2.4",
        "svelte-preprocess": "^4.10.7",
        "tailwindcss": "^3.1.5",
        "tslib": "^2.3.1",
        "typegraphql-prisma": "^0.21.5",
        "typescript": "^4.7.4",
        "vite": "^3.1.0"
    },
    "type": "module"
}
